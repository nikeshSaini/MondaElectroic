<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Feedback Data</title>
</head>
<body>
    <h1>Feedback Data:<%= currUserName %></h1>
    <% if (feedbacks.length > 0) { %>
        <ul>
            <% feedbacks.forEach(function(feedback) { %>
                <li>
                    <strong>Venue Name:</strong> <%= feedback.venue %><br>
                    <strong>Description:</strong> <%= feedback.description %><br>
                    <strong>Expenditure:</strong> <%= feedback.expenditure %><br>
                    <strong>Location:</strong> <%= feedback.location %><br>
                    <!-- <img src="/uploads/<%= feedback.img %>" alt="<%= feedback.venue %> "> -->
                    <!-- You can include additional fields as needed -->
                    <hr>
                </li>
            <% }); %>
        </ul>
    <% } else { %>
        <p>No feedback data available.</p>
    <% } %>
</body>
</html>
